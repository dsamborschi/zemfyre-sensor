{
  "name": "billing-exporter",
  "version": "1.0.0",
  "description": "Kubernetes metrics exporter for Zemfyre billing system",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "watch": "tsc --watch",
    "start": "node dist/index.js",
    "dev": "tsx src/index.ts",
    "clean": "rimraf dist",
    "docker:build": "docker build -t iotistic/billing-exporter:latest .",
    "docker:push": "docker push iotistic/billing-exporter:latest"
  },
  "keywords": [
    "billing",
    "kubernetes",
    "metrics",
    "prometheus"
  ],
  "dependencies": {
    "axios": "^1.12.2",
    "express": "^4.18.2",
    "dotenv": "^16.3.1",
    "winston": "^3.18.3"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/node": "^20.19.22",
    "typescript": "^5.3.3",
    "tsx": "^4.7.0",
    "rimraf": "^5.0.5"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
