{
  "name": "@aws-iot/device-client-jobs",
  "version": "1.0.0",
  "description": "Node.js TypeScript implementation of AWS IoT Device Client Jobs Feature",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "build:watch": "tsc -w",
    "start": "node dist/index.js",
    "dev": "ts-node src/index.ts",
    "example:basic": "ts-node examples/basic-usage.ts",
    "example:windows": "ts-node examples/windows-example.ts",
    "example:basic-js": "npm run build && node -e \"require('./dist/examples/basic-usage.js')\"",
    "examples": "npm run example:basic",
    "test": "jest",
    "test:watch": "jest --watch",
    "lint": "eslint src/**/*.ts",
    "clean": "rimraf dist"
  },
  "keywords": [
    "aws",
    "iot",
    "device-client",
    "jobs",
    "mqtt",
    "typescript"
  ],
  "author": "AWS IoT Device Client Contributors",
  "license": "Apache-2.0",
  "dependencies": {
    "@types/node-cron": "^3.0.11",
    "aws-iot-device-sdk-v2": "^1.19.0",
    "mqtt": "^5.3.0",
    "node-cron": "^4.2.1",
    "uuid": "^9.0.0",
    "zod": "^3.22.0"
  },
  "devDependencies": {
    "@types/jest": "^29.0.0",
    "@types/node": "^20.0.0",
    "@types/uuid": "^9.0.0",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "eslint": "^8.57.1",
    "jest": "^29.0.0",
    "rimraf": "^5.0.0",
    "ts-jest": "^29.0.0",
    "ts-node": "^10.0.0",
    "typescript": "^5.0.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
