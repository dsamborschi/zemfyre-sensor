{
  "apps": [
    {
      "appName": "test-app",
      "services": [
        {
          "serviceName": "nodered",
          "imageName": "nodered/node-red:latest",
          "config": {
            "environment": {
              "TZ": "UTC",
              "NODE_RED_ENABLE_PROJECTS": "true"
            },
            "ports": [
              "1880:1880"
            ],
            "volumes": [
              "nodered-data:/data"
            ],
            "restart": "unless-stopped"
          }
        }
      ]
    }
  ],
  "config": {}
}
